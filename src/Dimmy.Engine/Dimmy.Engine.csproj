    <PackageReference Include="Microsoft.Win32.Registry" Version="6.0.0-preview.3.21201.4" />
    <PackageReference Include="NuGet.Protocol" Version="5.11.5" />
    <PackageReference Include="NuGet.Resolver" Version="5.10.0-preview.2.7185" />
    <PackageReference Include="Octostache" Version="2.9.4" />
    <PackageReference Include="SimpleInjector" Version="5.3.0" />
    <PackageReference Include="YamlDotNet" Version="11.1.3-nullable-enums-0003" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
    <Folder Include="SharpHostsFile" />
  </ItemGroup>

</Project>
